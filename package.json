{
  "name": "prizmdoc-sample-central-splash-page",
  "version": "0.0.0",
  "description": "Central Splash Page that tells you which version of the client is running and provides a link to all the other sample pages",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node_modules/.bin/babel-node --presets react,es2015 src/server/app-server.js",
    "start-dev": "npm run start-dev-hmr",
    "start-dev-single-page": "node_modules/.bin/http-server src/static",
    "start-dev-hmr": "cross-env NODE_ENV=development node_modules/.bin/babel-node --presets react,es2015  src/server/app-server.js",
    "build": "cross-env NODE_ENV=production node_modules/.bin/webpack --config webpack.config.prod.js -p",
    "deploy": "cross-env pm2 start --interpreter NODE_ENV=production node_modules/.bin/babel-node src/server/app-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git@git.jpg.com:cdavis/prizmdoc-sample-central-splash-page.git"
  },
  "keywords": [
    "react",
    "webpack",
    "prizmdoc",
    "samples",
    "es2015"
  ],
  "author": "cdavis",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "bluebird": "^3.4.7",
    "classnames": "^2.2.5",
    "css-modules-require-hook": "^4.0.5",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.3.2",
    "react-burger-menu": "^1.10.14",
    "react-dom": "^15.3.2",
    "react-easter-egg": "^0.1.1",
    "react-router": "^2.6.1"
  },
  "devDependencies": {
    "babel-loader": "^6.3.2",
    "babel-preset-react-hmre": "^1.1.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-config-cdavis": "^0.1.5",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.1",
    "http-server": "^0.9.0",
    "install": "^0.8.4",
    "postcss-loader": "^1.3.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.8",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.3"
  }
}
